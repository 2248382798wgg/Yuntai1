.pure_top {
	width: 100%;
	height: 43px;
	position: relative;
	z-index: 999;
	margin-top: -4%;
	overflow: hidden;
}
.pure_top::after {
	content: '';
	width: 140%;
	height: 100px;
	position: absolute;
	left: -20%;
	top: 0;
	z-index: -1;
	border-radius: 50% 50% 0 0;
	background: #FFFFFF;
}
/* 介绍 */

